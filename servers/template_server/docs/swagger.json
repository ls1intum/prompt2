{
    "swagger": "2.0",
    "info": {
        "description": "This is the template server of PROMPT.",
        "title": "PROMPT Template API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8086",
    "basePath": "/template-service/api",
    "paths": {
        "/copy": {
            "post": {
                "description": "Copy template server data from a source course phase to a target course phase.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "copy"
                ],
                "summary": "Copy course phase data",
                "parameters": [
                    {
                        "description": "Copy request",
                        "name": "payload",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/promptTypes.PhaseCopyRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/course_phase/{coursePhaseID}/config": {
            "get": {
                "description": "Get configuration status flags for a course phase.",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "config"
                ],
                "summary": "Get phase configuration status",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Course Phase UUID",
                        "name": "coursePhaseID",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "boolean"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/course_phase/{coursePhaseID}/hello": {
            "get": {
                "description": "Returns a simple hello message from the template server.",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "template"
                ],
                "summary": "Template server health check",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Course Phase UUID",
                        "name": "coursePhaseID",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/course_phase/{coursePhaseID}/info": {
            "get": {
                "description": "Get template-specific info for a course phase.",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "template"
                ],
                "summary": "Get template info",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Course Phase UUID",
                        "name": "coursePhaseID",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "promptTypes.PhaseCopyRequest": {
            "type": "object",
            "properties": {
                "sourceCoursePhaseID": {
                    "description": "SourceCoursePhaseID is the unique identifier of the course phase to copy from.\nThis phase serves as the template and its data will be duplicated.",
                    "type": "string"
                },
                "targetCoursePhaseID": {
                    "description": "TargetCoursePhaseID is the unique identifier of the course phase to copy into.\nThis phase will receive the copied data and configurations.",
                    "type": "string"
                }
            }
        }
    },
    "externalDocs": {
        "description": "PROMPT Documentation",
        "url": "https://ls1intum.github.io/prompt2/"
    }
}